<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.demoaltp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.demoaltp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.demoaltp.RegisterActivity"
            android:label="ACTIVITY_REGISTER" >
        </activity>
        <activity
            android:name="com.example.demoaltp.AllFeature"
            android:label="ALL_FEATURE" >
        </activity>
        <activity
            android:name="com.example.demoaltp.GameActivity"
            android:label="GameACtivity" >
        </activity>
        <activity
            android:name="com.example.demoaltp.ListUserActivity"
            android:label="ListUserActivity" >
        </activity>
    </application>

</manifest>